# Cloudflare API credentials
CLOUDFLARE_API_TOKEN=your_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Namecheap API credentials
NAMECHEAP_API_KEY=your_api_key_here
NAMECHEAP_USERNAME=your_username_here

# GoDaddy API credentials
GODADDY_API_KEY=your_api_key_here
GODADDY_API_SECRET=your_api_secret_here

# Submission settings
MAX_URLS_PER_RUN=100
REQUEST_DELAY=5
GOOGLE_SEARCH_LIMIT=10
DRISSIONPAGE_TIMEOUT=30

# Google Search Console API settings
USE_GSC_API=false
SUBMIT_SITEMAP=false
GOOGLE_SERVICE_ACCOUNT_FILE=service_account.json
# Alternative to service account file, you can use these environment variables:
GOOGLE_SERVICE_ACCOUNT_PRIVATE_KEY=your_private_key_here
GOOGLE_SERVICE_ACCOUNT_CLIENT_EMAIL=your_client_email_here

# Bing Webmaster Tools API settings
BING_API_KEY=your_bing_api_key_here

# Yandex Webmaster API settings
YANDEX_API_KEY=your_yandex_api_key_here

# License key for premium features
LICENSE_KEY=your_license_key_here

# Enterprise features - Cloudflare Worker
CLOUDFLARE_WORKER_URL=https://auto-submit.your-worker.workers.dev
GITHUB_TOKEN=your_github_token_here