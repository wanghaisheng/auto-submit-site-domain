# Cloudflare API Configuration
CLOUDFLARE_API_TOKEN=your_cf_api_token_here
CLOUDFLARE_ACCOUNT_ID=your_cf_account_id_here
# Both API token and Account ID are required to fetch zones

# Sitemap Parser Settings
MAX_URLS_PER_RUN=100  # Set to 0 for unlimited
REQUEST_DELAY=2  # seconds between sitemap requests

# Google Submission Settings
REQUEST_DELAY=5  # seconds between Google submissions
GOOGLE_SEARCH_LIMIT=10  # max submissions per minute before longer pause
DRISSIONPAGE_TIMEOUT=30  # browser timeout in seconds